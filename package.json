{
  "name": "grow-your-need---visionary-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:client\" \"pnpm:dev:pocketbase\" \"pnpm:dev:ai\" \"pnpm:dev:server\" --names \"CLIENT,POCKETBASE,AI,SERVER\" --prefix-colors \"blue,magenta,yellow,green\"",
    "dev:client": "vite",
    "dev:pocketbase": "cd pocketbase && pocketbase.exe serve --http=127.0.0.1:8090",
    "dev:ai": "powershell -ExecutionPolicy Bypass ./launch-ai.ps1",
    "dev:server": "cd server && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lhci": "lhci autorun",
    "db:setup-files": "node scripts/maintenance/create-file-uploads-collection.js",
    "db:seed-materials": "node scripts/maintenance/seed-course-materials.js",
    "db:seed-all": "node scripts/maintenance/seed-all-data.js",
    "db:backup": "node scripts/maintenance/create-backup.js",
    "prod:check": "node scripts/prod-check.js"
  },
